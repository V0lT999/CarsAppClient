
@{
    ViewBag.Title = "title";
    Layout = "_Layout";
}

<h2>Add Dealer</h2>
<form method="post" action="/Dealer/Put">
    <div class="form-group">
        <label for="Brand">Brand <small class="text-danger"> (requied)</small></label>
        <input type="text" name="Brand" class="form-control" id="Brand" aria-describedby="emailHelp" required>
        @* <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small> *@
    </div>
    <div class="form-group">
        <label for="City">City <small class="text-danger"> (requied)</small></label>
        <input type="text" name="City" class="form-control" id="City" required>
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>